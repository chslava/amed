ul.list, ol.list {
  list-style: none;
  padding:1em 0;
  li {
    margin-bottom:15px;
    &:before {
      content: "\00a0";
      float:left;
    }
  }
}
ul.list {
  li {
    &:before {
      display: block;
      width: 13px;
      height:13px;
      background: #ff6b01;
      margin-top:0.3em;
      margin-right:9px;
    }
  }
}
ol.list {
  counter-reset: li;
  li {
    counter-increment: li;
    list-style: none;
    &:before {
      display: block;
      content: counter(li) '';
      background:#FFF;
      width: 20px;
      height: 20px;
      border:1px solid #ff6b01;
      color: #ff6b01;
      margin-right:15px;
      text-align: center;
      font-size:13px;
      line-height: 20px;
      list-style-position: inside;
    }
  }
}

blockquote {
  border-left: 2px solid #e5702a;
  padding-left: 17px;
  margin-left: 0;
}


.ui.title {
  min-height: 27px;
  color: #414141;
  font-size: 22px;
  font-weight: 700;
