}




.circles-list {
  list-style: none;
  padding:1em 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  li {
    width: 100%;
    min-height: 70px;
    align-self: flex-start;
    line-height: 65px;
    &:before {
      content: "\e905";
      font-family: 'icomoon' !important;
      width: 60px;
      height: 60px;
      display: block;
      border-radius: 50px;
      background: #FFF;
      float: left;
      margin-right: 20px;
      color: #e5702a;
      text-align: center;
     // padding-top: 2px;
      box-shadow: 0 6px 10px rgba(27, 47, 78, 0.14);
    }
  }
  &.small {
    li {
      min-height:55px;
      line-height: 40px;
      &:before {
        width: 40px;
        height: 40px;
        font-size:13px;
      }
    }
  }

}

.circle-check {
  margin-right: 20px;
  width: 60px;
  height: 60px;
  box-sizing: border-box;
  float: left;
  display: block;
  border-radius: 100px;
  box-shadow: 0 6px 10px rgba(27, 47, 78, 0.14);
  background: #FFF;
  text-align:center;
  flex-grow: 0 !important;
  -webkit-flex-grow: 0 !important;
  margin-bottom:10px;
  i {
    margin-top: 23px;
    display: block;
    color: #e5702a;
  }
}

