.contact-form {
  margin-top:1rem;
  padding:100px 0;
}
// -----------------------------------------
footer {
  background: #636463;
  color: #FFF;
  a {
    color: #FFF;
  }

  nav {
    > div {
      border-bottom:1px solid #8f8f8f;
      padding: 25px 0;
    }
  }

  .ui.text.menu {
    a.item, .item {
      color: #FFF;
      margin-bottom:14px;
    }
  }

  .soc-icon {
    font-size:30px;
    margin-right:20px;
    color: #d8d8d8;
    margin-top:-2px;
    display: block;
    float:left;
  }

  .c-icon {
    color: #8f8f8f;
    font-size: 18px;
    min-height: 30px;
    float: left;
    display: block;
    margin-right: 14px;
    min-width: 24px;
    line-height: 24px;
  }

  .footer-contacts, .follow-us {
    div.title {
      text-transform: uppercase;
      height: 25px;
      color: white;
      font-size: 15px;
      font-weight: 500;
      line-height: 25px;
    }
  }
  .follow-us {
    a.item {

    }
    a.item:hover .soc-icon {
      color: #ff6b01;
    }
  }

  .copyright {
    background: #414141;
    font-size: 12px;
    font-weight: 500;
    line-height: 15px;
    text-align: center;
    padding:18px 0;
  }
  // tabled >
  @media (min-width: 768px) {
    nav {
      > div {
        width: 100%;
        border-bottom:1px solid #8f8f8f;
      //  margin:0 auto;
        column-gap: 80px;
        columns: 2;
        padding: 25px 0;
      }
    }

    .footer-contacts .menu{
      .item {
        line-height: 25px;
      }

    }
    .follow-us {
      .ui.menu {
        display: block;
      }

    }

    .copyright {
      background: #636463!important;
      color: #8f8f8f;
      line-height: 18px;
      padding:20px 0;
      font-size:15px;
    }
  }
  // computer >
  @media only screen and (min-width: 992px) {
    nav {
      > div {
        border-right:1px solid #8f8f8f;
        width: 100%;
        border-bottom:none;
        margin:0;
        column-gap: 80px;
        columns: 2;
        padding: 0;
      }
    }
    .footer-contacts {
      margin-top:1rem;
      > .ui.menu {

        width: auto;
        border-right:1px solid #8f8f8f;
        min-height: 195px;
      }
    }
    .follow-us {
      flex-direction: column;
      margin-top:1rem;
    }
    .copyright {
      height: 25px;
      line-height: 25px;
      padding:30px 0;
    }
  }
}
