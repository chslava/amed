{
  "base": "ui",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "./../dist/ui/",
      "uncompressed": "./../dist/ui/components/",
      "compressed": "./../dist/ui/components/compressed/",
      "themes": "./../dist/ui/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "input",
    "segment",
    "breadcrumb",
    "form",
    "grid",
    "menu",
    "table",
    "checkbox",
    "dropdown",
    "rating",
    "transition",
    "form",
    "visibility",
    "accordion"
  ],
  "version": "2.2.4"
}